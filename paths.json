{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@*": [
        "src/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@com/*": [
        "src/components/*"
      ],
      "@util/*": [
        "src/utils/*"
      ],
      "@il8n/*": [
